#include <iostream>

using namespace std;

int main() {
	int numeros[100],n;

	cout << "\nDigite el numero de elementos que va a tener el arreglo: ";
	cin >> n;

	for (int i = 0; i < n; i++) {
		cout << "Digite un numero: ";
		cin >> numeros[i];  
	}
	cout << "\nLos numeros invertidos son: ";
	for (int i = n-1; i>=0; i--) {
		cout<< "\n" <<numeros[i] << endl;
	}

	return 0;
}
