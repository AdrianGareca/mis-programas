#include <iostream>

using namespace std;

int main()
{
    int num1[5];
    int num2[5];

    for (int i = 0; i < 5; i++) {
        cout << i + 1 << "Digite un numero: ";
        cin>>num1[i];
    }
    for (int i = 0; i < 5; i++) {
        num2[i] = num1[i]*2;
    }

    cout << "Los numeros multiplicados son:" << endl;

    for (int i = 0; i < 5; i++) {
        cout << num2[i] << endl;
    }

    return 0;
}
