import time

print("Te dire si un numero es primo o no.")
num=int(input("ingresa un numero: "))
if num<num3:
    ("Solo positivos mayores a 2")
else:
    inicio=time.time()
    div=2
    limite=round(num/2)
    bandera=False
    while div<=limite:
        if num%div==0:
            bandera=True
                break
        div+=1
    if bandera:
        print("NO es primo")
    else:
        print("SI es primo")
    final=time.time()-inicio
    print("Se tardo "+str(int(final*1000)))

