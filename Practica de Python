import random

lista=[]

for x in range(20):
    lista.append(int(random.random()*100))

def intercambio(p1,p2):
        aux =lista[p2]           #paso 1
        lista[p2]=lista[p1]       #paso 2
        lista[p1]=aux            #paso 3
    

for i in range(1,len(lista)):
    if lista[0]<lista[i]:
        intercambio(0,i)
        
        
print(lista)

