import time

print("Te dire si un numero es primo o no.")
num=int(input("ingresa un numero: "))
inicio=time.time()
div=round(num/2)
bandera=False
while div>1:
    if num%div==0:
        bandera=True
        break
    div-=1
if bandera:
    print("NO es primo")
else:
    print("SI es primo")
final=time.time()-inicio
print("Se tardo "+str(int(final*1000)))

