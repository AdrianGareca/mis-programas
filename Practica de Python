#bubble.py


import random

lista=[]

for x in range(20):
    lista.append(int(random.random()*100))

for a in range(0,len(lista)-1):
    for i in range(a+1,len(lista)):
        if lista[a]<lista[i]:
            lista[a],lista[i]=lista[i],lista[a]
        

print(lista)

-------------------------------------------------------------------------------------------------------------------------------------------------------------

def comp(texto):
    ctd=1
    actual=texto[0]
    salida=""

    for aux in texto[1:]:
        if aux==actual:
            ctd+=1
        else:
            salida+=str(ctd)
            salida+=actual
            ctd=1
            actual=aux
    salida+=str(ctd)
    salida+=actual
    return salida

t=input("Ingrese su texto a comprimir: ")
print(comp(t))

