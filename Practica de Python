import random

lista=[]

for x in range(20):
    lista.append(int(random.random()*100))

for i in range(1,len(lista)):
    if lista[0]<lista[i]:
        #usando un swap
        aux =lista[i]           #paso 1
        lista[i]=lista[0]       #paso 2
        lista[0]=aux            #paso 3
        
print(lista)

