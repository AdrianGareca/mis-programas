    if opc == "0":
        break
    elif opc == "1":
        log = input("ingresa tu log: ")
        pas = input("ingresa tu passwor: ")
        data.append({"l":log,"p":pas,"m":[]})
    elif opc == "2":
        l = input("Login: ")
        p = input("Psassword: ")
        for aux in data:
            if aux["l"] == l and aux["p"] == p:
                while True:
                    print("-"*20)
                    print("Bienvenido " + aux['p'])
                    print("1: Enviar mensaje")
                    print("2: Leer mensajes")
                    print("0: Cerrar Sesion")
