import random

def es_full(v):
    v=ordenar(v)
    res=[]
    actual=v[0]
    ctd=1
    for aux in v[1:]:
        if actual==aux:
            ctd+=1
        else:
            actual=aux
            res.append(ctd)
            ctd=1
    res.append(ctd)
    if len(res)==2 and 3 in res:
        return True
    else:
        return False

def es_poker(v):
    v=ordenar(v)
    res=[]
    actual=v[0]
    ctd=1
    for aux in v[1:]:
        if actual==aux:
            ctd+=1
        else:
            actual=aux
            res.append(ctd)
            ctd=1
    res.append(ctd)
    if len(res)==1 and 4 in res:
        return True
    else:
        return False

def es_grande(v):
    v=ordenar(v)
    res=[]
    actual=v[0]
    ctd=1
    for aux in v[1:]:
        if actual==aux:
            ctd+=1
        else:
            actual=aux
            res.append(ctd)
            ctd=1
    res.append(ctd)
    if len(res)==1 and 4 in res:
        return True
    else:
        return False

def ordenar(v):
    for x in range(len(v)-1):
        for y in range(x,len(v)):
            if v[x]>v[y]:
                v[x],v[y]=v[y],v[x]
    return v

def lanzar():
    res=[]
    for x in range(5):
        res.append(int(random.random()*6)+1)
    return res

ctd=0
for x in range(1000):
    l=lanzar()
    if es_full(l):
        print("full->"+str(l))
        ctd+=1
    elif es_poker(l):
        print("poker->"+str(l))
        ctd +=1
print("salieron "+str(ctd)+ " fulls")
print("salieron "+str(ctd)+ " pokers")

